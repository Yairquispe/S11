package ejemplo2;

/**
 *
 * @author LAB-USR-LNORTE
 */
public class vehiculo {
    private String placa;
    private String modelo;
    private String marca; 
    private vehiculo algunvehiculo; 

    public vehiculo() {
    }

    public vehiculo(String placa, String modelo, String marca) {
        this.placa = placa;
        this.modelo = modelo;
        this.marca = marca;
        this.algunvehiculo = algunvehiculo;
    }

    public String getPlaca() {
        return placa;
    }

    public void setPlaca(String placa) {
        this.placa = placa;
    }

    public vehiculo getAlgunvehiculo() {
        return algunvehiculo;
    }

    public void setAlgunvehiculo(vehiculo algunvehiculo) {
        this.algunvehiculo = algunvehiculo;
    }

    public String getModelo() {
        return modelo;
    }

    public void setModelo(String modelo) {
        this.modelo = modelo;
    }

    public String getMarca() {
        return marca;
    }

    public void setMarca(String marca) {
        this.marca = marca;
    }

    @Override
    public String toString() {
        return "/n informacion vehiculo"+
                "placa:"+this.placa+
                "modelo"+this.modelo
        + "marca"+this.marca ; 
    }
    
                
}
